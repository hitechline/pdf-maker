{
  "name": "@hitechline/pdf-maker",
  "version": "1.0.0-canary.3",
  "license": "MIT",
  "main": "dist/index.cjs.js",
  "module": "dist/index.esm.js",
  "types": "typings/index.d.ts",
  "scripts": {
    "fix": "yarn lint:fix && yarn prettier:fix",
    "release": "semantic-release",
    "prepack": "yarn build",
    "prepublishOnly": "yarn build",
    "test": "jest --passWithNoTests",
    "test:watch": "yarn test --watchAll",
    "clean": "rimraf .tarball coverage dist typings",
    "clean:build": "rimraf dist typings",
    "lint": "eslint ./ --ext js,jsx,ts",
    "lint:fix": "eslint ./ --ext js,jsx,ts --fix",
    "prettier": "prettier --check ./**/*.{js,jsx,ts}",
    "prettier:fix": "prettier --write ./**/*.{js,jsx,ts}",
    "build": "run-s build:{check,types,cjs,esm}",
    "prebuild": "yarn clean:build",
    "build:check": "tsc --noEmit",
    "build:types": "tsc -p build/tsconfig.types.json",
    "build:cjs": "node build/cjs",
    "build:esm": "node build/esm"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hitechline/pdf-maker.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "directories": {
    "lib": "src"
  },
  "files": [
    "dist",
    "typings"
  ],
  "keywords": [
    "react",
    "pdf",
    "maker",
    "handlebars",
    "puppeteer"
  ],
  "engines": {
    "node": ">=12"
  },
  "peerDependencies": {
    "handlebars": "^4.7.7"
  },
  "peerDependenciesMeta": {
    "handlebars": {
      "optional": true
    }
  },
  "dependencies": {
    "puppeteer": "^15.0.0"
  },
  "devDependencies": {
    "@hitechline/eslint-config": "2.0.2",
    "@hitechline/semantic-release-config": "1.2.0",
    "@jest/globals": "^28.0.0",
    "@semantic-release/changelog": "6.0.1",
    "@semantic-release/commit-analyzer": "9.0.2",
    "@semantic-release/git": "10.0.1",
    "@semantic-release/github": "8.0.4",
    "@semantic-release/npm": "9.0.1",
    "@semantic-release/release-notes-generator": "10.0.3",
    "@types/jest": "^28.0.0",
    "@types/jest-image-snapshot": "^4.3.1",
    "@types/node": "16.9.1",
    "@typescript-eslint/eslint-plugin": "5.29.0",
    "@typescript-eslint/parser": "5.29.0",
    "conventional-changelog-angular": "5.0.13",
    "conventional-changelog-conventionalcommits": "5.0.0",
    "esbuild": "0.14.47",
    "eslint": "8.18.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-import-resolver-typescript": "2.7.1",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-prettier": "4.0.0",
    "handlebars": "^4.7.7",
    "jest": "^28.0.0",
    "jest-image-snapshot": "^5.0.0",
    "npm-run-all": "4.1.5",
    "prettier": "2.7.1",
    "rimraf": "3.0.2",
    "semantic-release": "17.4.7",
    "ts-jest": "^28.0.0",
    "typescript": "4.7.4"
  }
}
